{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "object-curly-newline": 0,
    "object-property-newline": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-else-return": 0,
    "no-console": 0,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-underscore-dangle": 0,
    "arrow-parens": [
      "error",
      "always"
    ],
    "arrow-body-style": 0,
    "no-shadow": [
      "error",
      {
        "allow": [
          "done",
          "res",
          "cb",
          "err",
          "resolve",
          "reject"
        ]
      }
    ],
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "max-len": 0,
    "no-param-reassign": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "radix": 0,
    "no-plusplus": 0
  },
  "plugins": [
    "import"
  ]
}